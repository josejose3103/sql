SELECT
    A.[農家名],
    A.[病名],
    A.[金額] AS [平成15年金額],
    A.[日付] AS [平成15年日付],
    B.[フィールド２] AS [診療内容],
    B.[フィールド３] AS [保険外金額],
    B.[フィールド１] AS [保険外日付]
FROM
    [平成１５年] AS A
INNER JOIN
    [保険外 薬品] AS B
ON
    A.[農家名] = B.[フィールド４]
WHERE
    A.[日付] BETWEEN #2004/01/01# AND #2004/12/31#
    AND B.[フィールド１] BETWEEN #2011/01/01# AND #2011/12/31#;
